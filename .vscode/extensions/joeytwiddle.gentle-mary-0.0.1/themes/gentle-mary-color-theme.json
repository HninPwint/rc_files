{
  "name": "Gentle Mary",
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#102626",
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#D4B87E"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Modules",
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type",
      "scope": "support.type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Type",
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#3DA676",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Class method",
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "Function definition",
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Modifier",
      "scope": "modifier",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#D4B87E"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#CCCCCC",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "INI property name",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "INI section title",
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#AF4A67",
        "fontStyle": "bold"
      }
    },
    {
      "name": "C# class",
      "scope": "source.cs meta.class.identifier storage.type",
      "settings": {
        "foreground": "#3DA676",
        "fontStyle": "underline"
      }
    },
    {
      "name": "C# class method",
      "scope": "source.cs meta.method.identifier entity.name.function",
      "settings": {
        "foreground": "#4B95DC"
      }
    },
    {
      "name": "C# function call",
      "scope": "source.cs meta.method-call meta.method, source.cs entity.name.function",
      "settings": {
        "foreground": "#3DA676"
      }
    },
    {
      "name": "C# type",
      "scope": "source.cs storage.type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# return type",
      "scope": "source.cs meta.method.return-type",
      "settings": {
        "foreground": "#A576A0"
      }
    },
    {
      "name": "C# preprocessor",
      "scope": "source.cs meta.preprocessor",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "C# namespace",
      "scope": "source.cs entity.name.type.namespace",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Global settings",
      "settings": {
        "background": "#102626",
        "foreground": "#CCCCCC"
      }
    }
  ],
  "colors": {
    "focusBorder": "#CCCCCC",
    "foreground": "#CCCCCC",
    "dropdown.background": "#193c3c",
    "input.background": "#193c3c",
    "inputOption.activeBorder": "#CCCCCC",
    "list.activeSelectionBackground": "#CCCCCC80",
    "list.dropBackground": "#CCCCCC80",
    "list.focusBackground": "#CCCCCC80",
    "list.hoverBackground": "#FFFFFF1a",
    "list.inactiveSelectionBackground": "#FFFFFF33",
    "activityBar.background": "#163535",
    "activityBar.dropBackground": "#CCCCCC80",
    "activityBarBadge.background": "#CCCCCC",
    "sideBar.background": "#132d2d",
    "sideBarSectionHeader.background": "#163535",
    "editorGroup.dropBackground": "#CCCCCC80",
    "editorGroupHeader.tabsBackground": "#132d2d",
    "tab.border": "#00000033",
    "tab.inactiveBackground": "#163535",
    "editor.background": "#102626",
    "editor.foreground": "#CCCCCC",
    "editorLineNumber.foreground": "#FFFFFF4d",
    "editor.lineHighlightBackground": "#FFFFFF0a",
    "editor.rangeHighlightBackground": "#FFFFFF0d",
    "editorWidget.background": "#132d2d",
    "editorHoverWidget.background": "#132d2d",
    "editorMarkerNavigation.background": "#132d2d",
    "peekView.border": "#CCCCCC",
    "peekViewEditor.background": "#0c1e1e",
    "peekViewResult.background": "#132d2d",
    "peekViewTitle.background": "#102626",
    "panel.background": "#132d2d",
    "panel.border": "#FFFFFF1a",
    "panelTitle.activeBorder": "#CCCCCC80",
    "panelTitle.inactiveForeground": "#CCCCCC80",
    "statusBar.background": "#0c1e1e",
    "statusBar.debuggingBackground": "#CCCCCC",
    "statusBar.noFolderBackground": "#0c1e1e",
    "statusBarItem.activeBackground": "#CCCCCC80",
    "statusBarItem.hoverBackground": "#FFFFFF1a",
    "titleBar.activeBackground": "#0c1e1e",
    "pickerGroup.border": "#FFFFFF1a",
    "debugToolBar.background": "#163535"
  }
}
